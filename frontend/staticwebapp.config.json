{
    "routes": [
        {
          "route": "/login",
          "redirect": "/.auth/login/github"
        },
        {
            "route": "/",
            "allowedRoles": [ "admin" ]
        }
    ],
    "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      }
    }
}
